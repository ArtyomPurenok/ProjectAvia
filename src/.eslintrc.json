{
  "env": {
      "browser": true,
      "es6": true
  },
  "plugins": [
    "react", "import", "react-hooks", "prettier"
  ],
  "extends": [
    "eslint-config-react-app",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "settings": {
    "react": {
      "version": "17.0.2"
    }
  },
  "rules": {
      "eqeqeq": "warn",
      "strict": "off",
      "no-console": ["error",{ "allow": ["error"]}]




  }
}

